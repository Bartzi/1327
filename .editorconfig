# http://editorconfig.org

root = true

[*]
indent_style = tab
indent_size = tab
tab_width = 4
insert_final_newline = true
trim_trailing_whitespace = true
end_of_line = lf
charset = utf-8

# Minified JavaScript files shouldn't be changed
[**.min.js]
indent_style = ignore
insert_final_newline = ignore
